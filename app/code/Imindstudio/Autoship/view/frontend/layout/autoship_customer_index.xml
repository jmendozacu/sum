<?xml version="1.0"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../../../../../lib/internal/Magento/Framework/View/Layout/etc/page_configuration.xsd">
	<update handle="customer_account"/>
	<head>
		<link src="Imindstudio_Autoship::js/app.js"/>
	</head>
	<body>
		<referenceBlock name="page.main.title">
			<action method="setPageTitle">
				<argument translate="true" name="title" xsi:type="string">PENDING REVIEWS</argument>
			</action>
		</referenceBlock>
		
		<referenceContainer name="content">
			<block class="Imindstudio\Autoship\Block\Reviews" name="welcome" template="Imindstudio_Autoship::reviews.phtml" cacheable="false">
			</block>
		</referenceContainer>
	</body>
</page>
